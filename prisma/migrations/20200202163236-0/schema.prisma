datasource pg {
  provider = "postgresql"
  url = "***"
}

generator ts  {
  provider = "prisma-client-js"
}
 
model User {
  id              Int       @id @default(autoincrement())
  username        String 
  email           String    @unique               
}

model Post {
  id          Int        @id @default(autoincrement())
  title       String 
  published   Boolean    @default(false)
}